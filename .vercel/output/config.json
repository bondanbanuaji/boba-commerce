{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/_actions(?:/(.*?))?/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/auth/login/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/auth/register/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/cart/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/checkout/success/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/checkout/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/products/([^/]+?)/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/products/?$",
			"dest": "/_isr?x_astro_path=$0"
		},
		{
			"src": "^/$",
			"dest": "/_isr?x_astro_path=$0"
		}
	],
	"images": {
		"sizes": [
			640,
			750,
			828,
			1080,
			1200,
			1920,
			2048,
			3840
		],
		"domains": [],
		"remotePatterns": []
	}
}